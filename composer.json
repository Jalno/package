{
    "name": "jalno/package",
    "version": "0.0.1",
    "description": "Template package using Laravel Lumen Framework.",
    "keywords": ["jalno", "framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^7.4|^8.0",
        "jalno/lumen": "0.0.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Jalno\\Package\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
